#include <stdio.h>
#include <math.h>

int main() {
    int n,casas;
     double gramas=0.0,soma=0.0;
    scanf("%d",&n);
    for(int i=0;i<n;i++){
        scanf("%d",&casas);
        soma=(pow(2,casas)-1);
        gramas=soma/(12*1.0);
        gramas=gramas/(1000);
        gramas=floor(gramas);
        printf("%.f kg\n",gramas);
    }

    return 0;
}
