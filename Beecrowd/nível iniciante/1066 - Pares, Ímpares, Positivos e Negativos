#include <stdio.h>

int main() {
    int a,b,c,d,e,par=0,impar=0,positivo=0,negativo=0;
    scanf("%d%d%d%d%d",&a,&b,&c,&d,&e);
    int vet[]={a,b,c,d,e};
    for(int i=0;i<5;i++){
        if(vet[i]%2==0)
            par++;
        else
            impar++;
        if(vet[i]>0)
            positivo++;
        else if(vet[i]<0)
            negativo++;
    }
    printf("%d valor(es) par(es)\n%d valor(es) impar(es)\n%d valor(es) positivo(s)\n%d valor(es) negativo(s)\n",par,impar,positivo,negativo);
    return 0;
}
