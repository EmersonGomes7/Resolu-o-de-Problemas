#include <stdio.h>

int main() {
    int n,dividendo,divisor;
    scanf("%d",&n);
    for(int i=0;i<n;i++){
        scanf("%d %d",&dividendo,&divisor);
        if(divisor==0)
            printf("divisao impossivel\n");
        else{
            printf("%.1f\n",(float)(dividendo*1.0/divisor));
        }
    }

    return 0;
}
