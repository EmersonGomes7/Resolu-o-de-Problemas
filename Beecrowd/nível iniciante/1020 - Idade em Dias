#include <stdio.h>

int main(){
    int idade,anos=0,meses=0;
    scanf("%d",&idade);
        do{
        if(idade>=365){
            anos++;
            idade-=365;
        }else if(idade>=30){
            meses++;
            idade-=30;
        }
    }while(idade>=30);
    
    printf("%d ano(s)\n%d mes(es)\n%d dia(s)\n",anos,meses,idade);

    return 0;
}
