#include <stdio.h>

int main() {
    int a,b,maior,menor,cont=0;
    scanf("%d%d",&a,&b);
    if(a>b){
        maior=a;
        menor=b;
    }else{
        maior=b;
        menor=a;
    }
    for(menor=menor+1;menor<maior;menor++){
        if((menor)%2!=0){
            cont+=menor;
        }

    }
    printf("%d\n",cont);

    return 0;
}
