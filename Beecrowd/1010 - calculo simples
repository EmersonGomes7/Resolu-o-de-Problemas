#include <stdio.h>

int main(){
    int codigo1,codigo2;
    int quantidade1;
    int quantidade2;
    double valor1;
    double valor2;
    scanf("%d %d %lf",&codigo1,&quantidade1,&valor1);
    getchar();
    scanf("%d %d %lf",&codigo2,&quantidade2,&valor2);
    printf("VALOR A PAGAR: R$ %.2lf\n",valor1*quantidade1+valor2*quantidade2);
    return 0;
}
